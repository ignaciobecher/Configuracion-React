{
    "extends": [
        "eslint:recommended",
        "prettier",
        "prettier/react"
    ],

    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },

    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}